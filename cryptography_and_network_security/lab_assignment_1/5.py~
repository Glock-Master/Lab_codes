def gcd(a, b):
	if (b == 0):
		return a;
	else:
		return gcd(b, a % b);
		
		
def inverse_pairs_ext_euc(n):

	mul = set()
	for i in range(1, n):
		if (gcd(n, i) != 1):
			continue
		else:
		

n = int(input("Enter n to add. and mul. inverse pairs using ext. euclidean algo.: "))
inverse_pairs_ext_euc(n);
